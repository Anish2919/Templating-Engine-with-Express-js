<div class="container">
    <h1>List of Marvel Movies</h1>

    {{> favMovies}}

    <div class="dislikedMovies">
        {{> dislikedMovies}}
    </div>

    <h2>Suggested Movies</h2>
    <ul>{{#each suggestedMovies }}
        <li class="{{ this.colors }}">{{ this.name }}</li>
        {{/each }}
    </ul>
</div>